<?xml version="1.0" encoding="UTF-8"?>
<configuration>
        <property name="LOG_PATTERN" value="[DEMO-SRV]-%d{yyyyMMdd HH:mm:ss}  [%-5p] - %m - %C - %M - %L - %n"/>
       
        <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
                <encoder>
                        <pattern>${LOG_PATTERN}</pattern>
                </encoder>
        </appender>
     
     
     <logger name="es.afi.sta" level="DEBUG" />
        <root level="WARN">
                <appender-ref ref="CONSOLE" />
        </root>
</configuration>